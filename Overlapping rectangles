#include <bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define mp make_pair
#define ff first
#define ss second
using namespace std;


int main() {
    ll a,b,c,d,t,i,j;
    cin>>t;
    while(t--){
       ll x1,y1,x2,y2,x3,y3,x4,y4;
       cin>>x1>>y1>>x2>>y2;
       cin>>x3>>y3>>x4>>y4;
       if(y1<y4 || y2>y3 || x2<x3 || x1>x4){
           cout<<0<<endl;
       }else{
           cout<<1<<endl;
       }
    }
}
